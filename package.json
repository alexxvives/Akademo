{
  "name": "akademo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "pages:build": "npx @opennextjs/cloudflare build",
    "preview": "npx @opennextjs/cloudflare preview",
    "deploy": "npx @opennextjs/cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv",
    "db:migrate": "wrangler d1 migrations apply akademo-db --remote",
    "db:migrate:local": "wrangler d1 migrations apply akademo-db --local"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.14.0",
    "@prisma/adapter-d1": "^7.0.1",
    "@prisma/client": "^5.20.0",
    "bcryptjs": "^2.4.3",
    "hls.js": "^1.6.15",
    "next": "14.2.15",
    "plyr": "^3.8.3",
    "plyr-react": "^6.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.6.0",
    "ua-parser-js": "^1.0.38",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/ua-parser-js": "^0.7.39",
    "autoprefixer": "^10.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.4.47",
    "prisma": "^5.20.0",
    "tailwindcss": "^3.4.14",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
