{
  "name": "@akademo/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for AKADEMO",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./api": {
      "types": "./dist/api.d.ts",
      "default": "./dist/api.js"
    },
    "./models": {
      "types": "./dist/models.d.ts",
      "default": "./dist/models.js"
    },
    "./database": {
      "types": "./dist/database.d.ts",
      "default": "./dist/database.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
